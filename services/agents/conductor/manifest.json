{
  "name": "AGENT-CONDUCTOR",
  "vt_layer": "AGTL",
  "vt_module": "CONDUCTOR",
  "version": "1.0.0",
  "description": "Conductor agent for orchestrating multi-agent workflows",
  "provider_policy": {
    "conductor": "gemini-pro",
    "fallback": "claude-sonnet-4"
  },
  "telemetry": {
    "emit_ready": true,
    "emit_heartbeat": true,
    "heartbeat_interval_seconds": 60
  },
  "runtime": {
    "type": "cloud-run",
    "min_instances": 1,
    "max_instances": 5
  },
  "dependencies": {
    "gateway": "https://vitana-gateway-86804897789.us-central1.run.app",
    "oasis": "https://vitana-gateway-86804897789.us-central1.run.app/api/v1/oasis"
  }
}
